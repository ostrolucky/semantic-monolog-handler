{
    "name": "ostrolucky/semantic-monolog-handler",
    "description": "Opinionated Monolog FingersCrossed handler which bypasses fingers-crossed behaviour for some primary channels",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Gabriel OstroluckÃ½",
            "email": "gabriel.ostrolucky@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "monolog/monolog": "^3"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.47",
        "infection/infection": "^0.29.14",
        "phpro/grumphp-shim": "^2.12",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12",
        "symplify/easy-coding-standard": "^12"
    },
    "autoload": {
        "psr-4": {
            "Ostrolucky\\SemanticMonologHandler\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true,
            "phpro/grumphp-shim": true
        },
        "sort-packages": true
    }
}
